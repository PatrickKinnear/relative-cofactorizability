
@article{ben-zviQuantumCharacterVarieties2018,
  title = {Quantum Character Varieties and Braided Module Categories},
  author = {{Ben-Zvi}, David and Brochier, Adrien and Jordan, David},
  year = {2018},
  month = jun,
  journal = {arXiv:1606.04769 [math]},
  eprint = {1606.04769},
  eprinttype = {arxiv},
  primaryclass = {math},
  abstract = {We compute quantum character varieties of arbitrary closed surfaces with boundaries and marked points. These are categorical invariants \$\textbackslash int\_S\textbackslash mathcal A\$ of a surface \$S\$, determined by the choice of a braided tensor category \$\textbackslash mathcal A\$, and computed via factorization homology. We identify the algebraic data governing marked points and boundary components with the notion of a \{\textbackslash em braided module category\} for \$\textbackslash mathcal A\$, and we describe braided module categories with a generator in terms of certain explicit algebra homomorphisms called \{\textbackslash em quantum moment maps\}. We then show that the quantum character variety of a decorated surface is obtained from that of the corresponding punctured surface as a quantum Hamiltonian reduction. Characters of braided \$\textbackslash mathcal A\$-modules are objects of the torus category \$\textbackslash int\_\{T\^2\}\textbackslash mathcal A\$. We initiate a theory of character sheaves for quantum groups by identifying the torus integral of \$\textbackslash mathcal A=\textbackslash operatorname\{Rep\_q\} G\$ with the category \$\textbackslash mathcal D\_q(G/G)-\textbackslash operatorname\{mod\}\$ of equivariant quantum \$\textbackslash mathcal D\$-modules. When \$G=GL\_n\$, we relate the mirabolic version of this category to the representations of the spherical double affine Hecke algebra (DAHA) \$\textbackslash mathbb\{SH\}\_\{q,t\}\$.},
  archiveprefix = {arXiv},
  keywords = {Mathematics - Algebraic Geometry,Mathematics - Quantum Algebra,Mathematics - Representation Theory},
  file = {/home/s1524448/Zotero/storage/78FUSHMU/Ben-Zvi et al_2018_Quantum character varieties and braided module categories.pdf;/home/s1524448/Zotero/storage/EELPB265/1606.html}
}

@article{brochierDualizabilityBraidedTensor2018,
  title = {On Dualizability of Braided Tensor Categories},
  author = {Brochier, Adrien and Jordan, David and Snyder, Noah},
  year = {2018},
  month = aug,
  journal = {arXiv:1804.07538 [math]},
  eprint = {1804.07538},
  eprinttype = {arxiv},
  primaryclass = {math},
  abstract = {We study the question of dualizability in higher Morita categories of locally presentable tensor categories and braided tensor categories. Our main results are that the 3-category of rigid tensor categories with enough compact projectives is 2-dualizable, that the 4-category of rigid braided tensor categories with enough compact projectives is 3-dualizable, and that (in characteristic zero) the 4-category of braided fusion categories is 4-dualizable. Via the cobordism hypothesis, this produces respectively 2, 3 and 4-dimensional framed local topological field theories. In particular, we produce a framed 3-dimensional local TFT attached to the category of representations of a quantum group at any value of q.},
  archiveprefix = {arXiv},
  langid = {english},
  keywords = {17B37; 18D10; 16D90; 57M27,Mathematics - Category Theory,Mathematics - Quantum Algebra},
  file = {/home/s1524448/Zotero/storage/6LECIB39/Brochier et al. - 2018 - On dualizability of braided tensor categories.pdf}
}

@article{brochierInvertibleBraidedTensor2020,
  title = {Invertible Braided Tensor Categories},
  author = {Brochier, Adrien and Jordan, David and Safronov, Pavel and Snyder, Noah},
  year = {2020},
  month = mar,
  journal = {arXiv:2003.13812 [math]},
  eprint = {2003.13812},
  eprinttype = {arxiv},
  primaryclass = {math},
  abstract = {We prove that a finite braided tensor category A is invertible in the Morita 4category BrTens of braided tensor categories if, and only if, it is non-degenerate. This includes the case of semisimple modular tensor categories, but also non-semisimple examples such as categories of representations of the small quantum group at good roots of unity. Via the cobordism hypothesis, we obtain new invertible 4-dimensional framed topological field theories, which we regard as a non-semisimple framed version of the Crane-Yetter-Kauffman invariants, after Freed\textendash Teleman and Walker's construction in the semisimple case. More generally, we characterize invertibility for E1- and E2algebras in an arbitrary symmetric monoidal {$\infty$}-category, and we conjecture a similar characterization of invertible En-algebras for any n. Finally, we propose the Picard group of BrTens as a generalization of the Witt group of non-degenerate braided fusion categories, and pose a number of open questions about it.},
  archiveprefix = {arXiv},
  langid = {english},
  keywords = {Mathematics - Quantum Algebra},
  file = {/home/s1524448/Zotero/storage/8YBQL5PL/Brochier et al. - 2020 - Invertible braided tensor categories.pdf}
}

